      },
      {
        "language": "javascript",
        "path": "./snippets/text-input.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/button.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/back-link.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/breadcrumbs.code-snippets"
